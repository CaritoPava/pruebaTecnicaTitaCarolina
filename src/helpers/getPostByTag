export const getPostByTag = async (tag) => {
  let postData
  console.log(tag)
  try {
    const url = `https://dummyapi.io/data/v1/tag/${tag.trim()}/post?limit=10`
    const resp = await fetch(url, {
      headers: {
        'app-id': '62676448c7b353234cb37a15'
      }
    })
    console.log(resp)
    const { data } = await resp.json()
    postData = data
  } catch (error) {
    console.log(error)
  }
  return postData
}